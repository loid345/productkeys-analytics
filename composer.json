{
    "name": "dart/productkeys-analytics",
    "description": "Dart Product Keys Analytics module for Magento 2",
    "require": {
        "php": ">=7.3"
    },
    "type": "magento2-module",
    "version": "0.1.0",
    "license": ["OSL-3.0", "AFL-3.0"],
    "autoload": {
        "files": [
            "registration.php"
        ],
        "psr-4": {
            "Dart\\ProductkeysAnalytics\\": ""
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Dart\\ProductkeysAnalytics\\Test\\": "Test/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^9.6"
    },
    "scripts": {
        "lint": "find . -name \"*.php\" -not -path \"./vendor/*\" -print0 | xargs -0 -n1 php -l",
        "ci": [
            "@lint",
            "composer validate --no-check-publish"
        ]
    }
}
