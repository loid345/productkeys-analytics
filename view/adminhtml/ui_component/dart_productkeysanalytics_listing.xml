<?xml version="1.0"?>
<listing xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Ui:etc/ui_configuration.xsd">
    <argument name="data" xsi:type="array">
        <item name="js_config" xsi:type="array">
            <item name="provider" xsi:type="string">dart_productkeysanalytics_listing.dart_productkeysanalytics_listing_data_source</item>
            <item name="deps" xsi:type="string">dart_productkeysanalytics_listing.dart_productkeysanalytics_listing_data_source</item>
        </item>
        <item name="spinner" xsi:type="string">dart_productkeysanalytics_columns</item>
    </argument>
    <dataSource name="dart_productkeysanalytics_listing_data_source">
        <argument name="dataProvider" xsi:type="configurableObject">
            <argument name="class" xsi:type="string">Dart\ProductkeysAnalytics\Ui\DataProvider\AnalyticsDataProvider</argument>
            <argument name="name" xsi:type="string">dart_productkeysanalytics_listing_data_source</argument>
            <argument name="primaryFieldName" xsi:type="string">sku</argument>
            <argument name="requestFieldName" xsi:type="string">sku</argument>
            <argument name="data" xsi:type="array">
                <item name="config" xsi:type="array">
                    <item name="update_url" xsi:type="url" path="mui/index/render"/>
                    <item name="storageConfig" xsi:type="array">
                        <item name="indexField" xsi:type="string">sku</item>
                    </item>
                </item>
            </argument>
        </argument>
    </dataSource>
    <listingToolbar name="listing_top">
        <bookmark name="bookmarks"/>
        <columnsControls name="columns_controls"/>
        <filterSearch name="fulltext"/>
        <filters name="listing_filters">
            <filterInput name="sku">
                <settings>
                    <label translate="true">SKU</label>
                </settings>
            </filterInput>
            <filterInput name="product_name">
                <settings>
                    <label translate="true">Product Name</label>
                </settings>
            </filterInput>
            <filterDateRange name="order_date">
                <settings>
                    <dataScope>order_date</dataScope>
                    <label translate="true">Order Date</label>
                </settings>
            </filterDateRange>
        </filters>
        <chips name="chips"/>
        <paging name="listing_paging"/>
        <exportButton name="export_button"/>
    </listingToolbar>
    <listingToolbar name="listing_bottom">
        <paging name="listing_paging"/>
    </listingToolbar>
    <columns name="dart_productkeysanalytics_columns">
        <column name="sku">
            <settings>
                <label translate="true">SKU</label>
                <filter>text</filter>
                <sorting>asc</sorting>
            </settings>
        </column>
        <column name="product_name">
            <settings>
                <label translate="true">Product Name</label>
                <filter>text</filter>
            </settings>
        </column>
        <column name="total_keys">
            <settings>
                <label translate="true">Total Keys</label>
                <filter>text</filter>
                <sortable>false</sortable>
                <totalsLabel translate="true">Totals</totalsLabel>
            </settings>
        </column>
        <column name="sold_keys">
            <settings>
                <label translate="true">Sold</label>
                <filter>text</filter>
                <sortable>false</sortable>
            </settings>
        </column>
        <column name="free_keys">
            <settings>
                <label translate="true">Free</label>
                <filter>text</filter>
                <sortable>false</sortable>
            </settings>
        </column>
    </columns>
</listing>
