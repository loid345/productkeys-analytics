<?xml version="1.0"?>
<listing xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:Ui/etc/ui_configuration.xsd">
    <settings>
        <spinner>dart_productkeysanalytics_columns</spinner>
        <emptyText translate="true">No records found</emptyText>
        <showTotals>true</showTotals>
    </settings>
    <argument name="data" xsi:type="array">
        <item name="component" xsi:type="string">Magento_Ui/js/grid/listing</item>
        <item name="js_config" xsi:type="array">
            <item name="provider" xsi:type="string">dart_productkeysanalytics_listing.dart_productkeysanalytics_listing_data_source</item>
            <item name="deps" xsi:type="string">dart_productkeysanalytics_listing.dart_productkeysanalytics_listing_data_source</item>
        </item>
        <item name="spinner" xsi:type="string">dart_productkeysanalytics_columns</item>
    </argument>
    <dataSource name="dart_productkeysanalytics_listing_data_source">
        <argument name="dataProvider" xsi:type="configurableObject">
            <argument name="class" xsi:type="string">Dart\ProductkeysAnalytics\Ui\DataProvider\AnalyticsDataProvider</argument>
            <argument name="name" xsi:type="string">dart_productkeysanalytics_listing_data_source</argument>
            <argument name="primaryFieldName" xsi:type="string">id</argument>
            <argument name="requestFieldName" xsi:type="string">id</argument>
            <argument name="data" xsi:type="array">
                <item name="config" xsi:type="array">
                    <item name="component" xsi:type="string">Magento_Ui/js/grid/provider</item>
                    <item name="update_url" xsi:type="url" path="mui/index/render"/>
                    <item name="pageSize" xsi:type="number">20</item>
                    <item name="noDataMessage" xsi:type="string" translate="true">No data found.</item>
                    <item name="showTotals" xsi:type="boolean">true</item>
                    <item name="storageConfig" xsi:type="array">
                        <item name="indexField" xsi:type="string">id</item>
                    </item>
                </item>
            </argument>
        </argument>
    </dataSource>
    <listingToolbar name="listing_top">
        <bookmark name="bookmarks"/>
        <columnsControls name="columns_controls"/>
        <filterSearch name="fulltext"/>
        <filters name="listing_filters">
            <filterInput name="sku">
                <settings>
                    <label translate="true">SKU / Key Pool Name</label>
                </settings>
            </filterInput>
            <filterRange name="created_at">
                <settings>
                    <label translate="true">Created At</label>
                    <dataType>date</dataType>
                </settings>
            </filterRange>
        </filters>
        <chips name="chips"/>
        <paging name="listing_paging"/>
        <exportButton name="export_button"/>
    </listingToolbar>
    <columns name="dart_productkeysanalytics_columns">
        <settings>
            <showTotals>true</showTotals>
        </settings>
        <column name="sku">
            <settings>
                <label translate="true">SKU / Key Pool Name</label>
                <filter>text</filter>
                <sorting>asc</sorting>
                <totalsLabel translate="true">Total</totalsLabel>
            </settings>
        </column>
        <column name="total_keys">
            <settings>
                <label translate="true">Total Keys</label>
                <dataType>number</dataType>
                <filter>text</filter>
                <totals>sum</totals>
            </settings>
        </column>
        <column name="sold_keys">
            <settings>
                <label translate="true">Sold</label>
                <dataType>number</dataType>
                <filter>text</filter>
                <totals>sum</totals>
            </settings>
        </column>
        <column name="free_keys">
            <settings>
                <label translate="true">Free</label>
                <dataType>number</dataType>
                <filter>text</filter>
                <totals>sum</totals>
            </settings>
        </column>
    </columns>
</listing>
